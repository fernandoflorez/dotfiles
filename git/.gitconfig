	name = Fernando FloÃÅrez
	email = fernando@funciton.com
[core]
	excludesfile = ~/.gitignore_global
	pager = delta
[init]
	defaultBranch = dev
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
	side-by-side = true
[merge]
	conflictStyle = zdiff3
[diff]
	colorMoved = default
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
